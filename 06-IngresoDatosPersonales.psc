Proceso IngresoDatosPersonales
	
	Definir nombre, apellido Como Cadena;
	Definir dni Como Entero;
	
	Escribir "Ingrese nombre: ";
	Leer nombre;
	
	Escribir "Ingrese apellido: ";
	Leer apellido;
	
	Escribir "Ingrese DNI: ";
	Leer dni;
	
	Escribir "------------------------------";
	Escribir " Datos Personales ";
	Escribir " Nombre: ", nombre;
	Escribir " Apellido: ", apellido;
	Escribir " DNI: ", dni;
	Escribir "------------------------------";
	
FinProceso
