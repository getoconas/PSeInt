Proceso MenorDosNumeros
	
	Definir numero1, numero2 Como Entero;
	
	Escribir "Ingrese primer numero entero";
	Leer numero1;
	
	Escribir "Ingrese segundo numero entero";
	Leer numero2;
	
	Si numero1 < numero2 Entonces
		Escribir "El número ", numero1, " es menor que el número ", numero2;
	SiNo
		Escribir "El número ", numero2, " es menor que el número ", numero1;
	FinSi
	
FinProceso
