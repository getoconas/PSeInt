Proceso CompararCadenas
	
	Definir cadena1, cadena2 Como Cadena;
	
	Escribir "Ingrese la primer cadena ";
	Leer cadena1;
	
	Escribir "Ingrese la segunda cadena ";
	Leer cadena2;
	
	Si cadena1 == cadena2 Entonces
		Escribir "Las cadenas ingresadas son iguales";
	SiNo
		Escribir "Las cadenas ingresadas son distintas";
	FinSi
	
FinProceso
